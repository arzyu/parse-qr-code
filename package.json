{
  "name": "@arzyu/parse-qr-code",
  "version": "0.1.0",
  "description": "Parse QR code from image file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:debug": "node --inspect-brk -r ts-node/register src/index.ts",
    "prebuild": "rimraf dist # rm -rf dist",
    "build": "tsc",
    "build:standalone": "ncc build src/index.ts -o dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "jimp": "^0.16.2",
    "jsqr": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@vercel/ncc": "^0.34.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "license": "MIT"
}
